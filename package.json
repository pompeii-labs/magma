{
  "name": "@pompeii-labs/magma",
  "version": "1.7.3",
  "description": "The Typescript framework to build AI agents quickly and easily",
  "keywords": [
    "Agents",
    "Typescript",
    "AI",
    "Agentic",
    "LLM",
    "Workflows",
    "Middleware"
  ],
  "license": "Apache-2.0",
  "author": "Pompeii Labs, Inc.",
  "files": [
    "dist"
  ],
  "repository": "pompeii-labs/magma",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./decorators": {
      "types": "./dist/decorators.d.ts",
      "default": "./dist/decorators.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/types/index.js"
    }
  },
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "build": "npm run format ; tsup",
    "dev": "tsup --watch"
  },
  "devDependencies": {
    "@swc/core": "^1.5.1",
    "@types/node-cron": "^3.0.11",
    "@types/ws": "^8.5.12",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "prettier": "^3.2.5",
    "tsup": "^8.0.2",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "node-cron": "^3.0.3",
    "@anthropic-ai/sdk": "~0.39.0",
    "@google/generative-ai": "~0.24.0",
    "@types/express": "~5.0.1",
    "groq-sdk": "~0.15.0",
    "openai": "~4.86.2",
    "ws": "~8.18.0"
  }
}
